import { describe, it, expect } from 'vitest'

describe('示例测试', () => {
it('基础断言', () => {
expect(1 + 1).toBe(2)
expect('hello').toContain('ell')
expect([1, 2, 3]).toHaveLength(3)
})

it('异步测试', async () => {
const data = await Promise.resolve({ name: '测试' })
expect(data).toEqual({ name: '测试' })
})

it('对象匹配', () => {
const user = {
name: 'Alice',
age: 25,
address: {
city: 'Beijing',
},
}

expect(user).toMatchObject({
name: 'Alice',
address: {
city: 'Beijing',
},
})
})
})